<mvc:View xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m"
  controllerName="project1.controller.Material">
  <Page id="_IDGenPage2" title="Material Master" showNavButton="true" navButtonPress=".onNavBack">
    <content>
      <Table id="_IDGenTable2" growing="true" items="{md>/Material}">
        <columns>
          <Column id="_IDGenColumn10"><Text id="_IDGenText20" text="ID"/></Column>
          <Column id="_IDGenColumn11"><Text id="_IDGenText21" text="Code"/></Column>
          <Column id="_IDGenColumn12"><Text id="_IDGenText22" text="Description"/></Column>
          <Column id="_IDGenColumn13"><Text id="_IDGenText23" text="Std Price"/></Column>
          <Column id="_IDGenColumn14"><Text id="_IDGenText24" text="Sales Price"/></Column>
        </columns>
        <items>
          <ColumnListItem id="_IDGenColumnListItem3">
            <cells>
              <Text id="_IDGenText25" text="{md>ID}"/>
              <Text id="_IDGenText26" text="{md>materialCode}"/>
              <Text id="_IDGenText27" text="{md>materialDescription}"/>
              <Text id="_IDGenText28" text="{md>standardPrice}"/>
              <Text id="_IDGenText29" text="{md>salesPrice}"/>
            </cells>
          </ColumnListItem>
        </items>
      </Table>
    </content>
  </Page>
</mvc:View>
